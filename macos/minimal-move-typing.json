      "description": "Application to Sleep",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "application"
          },
          "to": [
            {
              "key_code": "eject",
              "modifiers": [
                "left_option",
                "left_command"
              ]
            }
          ]
        }
      ]
    },
